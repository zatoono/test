
public class Android extends Mp3Player{

	public static Android(){
		System.out.println("Android");
		SmartPhone android = new SmartPhone();
		android.play();
		android.stop();
		android.next();
		android.back();
		android.call();
		android.mail();
		android.photo();
		android.internet();
		System.out.println();
	}
}
